	</form>
	<table>
        <caption>用户列表</caption>
        <thead>
            <tr>
                <th>活动ID</th>
                <th>活动名称</th>
                <th>活动内容</th>
                <th>活动时间</th></tr>
        </thead>
        <tbody id="lis">
            <c:forEach items="${activity2 }" var="node">  
      <tr>
        <td><c:out value="${node.activity_id }"></c:out></td>
        <td><c:out value="${node.activity_name}"></c:out></td>
        <td><c:out value="${node.activity_content}"></c:out></td>
