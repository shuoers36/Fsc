package com.hfsc.model;

import java.text.DateFormat;
import java.util.Date;

public class Score {

	private int Score_id;
	
	private int Student_id;
	
	private String Subject_name;
	
	private float Score;
	
	private Date Score_time;
	
	private String timeToString;
	
	public String getTimeToString() {
		  timeToString = DateFormat.getDateInstance(DateFormat.DEFAULT).format(Score_time);
		
		return timeToString;
	}

	public int getScore_id() {
		return Score_id;
	}

	public void setScore_id(int score_id) {
		Score_id = score_id;
	}

	public int getStudent_id() {
		return Student_id;
	}

	public void setStudent_id(int student_id) {
		Student_id = student_id;
	}

	public String getSubject_name() {
		return Subject_name;
	}

	public void setSubject_name(String subject_name) {
		Subject_name = subject_name;
	}

	public float getScore() {
		return Score;
	}

	public void setScore(float score) {
		Score = score;
	}

	public Date getScore_time() {
		return Score_time;
	}

	public void setScore_time(Date score_time) {
		Score_time = score_time;
	}

	public void setTimeToString(String timeToString) {
		this.timeToString = timeToString;
	}
}
