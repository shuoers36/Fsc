package com.hfsc.model;

import java.sql.Date;

public class Homework {
	private int Homework_id;
	
	private int Subject_id;
	
	private int Class_id;
	
	private String Homework_content;
	
	private Date Homework_time;
	
	private int Homework_submit;
	
	
	public int getHomework_id() {
		return Homework_id;
	}

	public void setHomework_id(int homework_id) {
		Homework_id = homework_id;
	}

	public int getSubject_id() {
		return Subject_id;
	}

	public void setSubject_id(int subject_id) {
		Subject_id = subject_id;
	}

	public int getClass_id() {
		return Class_id;
	}

	public void setClass_id(int class_id) {
		Class_id = class_id;
	}

	public String getHomework_content() {
		return Homework_content;
	}

	public void setHomework_content(String homework_content) {
		Homework_content = homework_content;
	}

	public Date getHomework_time() {
		return Homework_time;
	}

	public void setHomework_time(Date homework_time) {
		Homework_time = homework_time;
	}

	public int getHomework_submit() {
		return Homework_submit;
	}

	public void setHomework_submit(int homework_submit) {
		Homework_submit = homework_submit;
	}

	
	
	
}
